{
  "name": "nova-logs",
  "version": "3.1.0",
  "description": "A bot for logging basic user activity on the Makeshift Discord guild.",
  "main": "dist/server.js",
  "engines": {
    "node": ">=16.11.0 <=20"
  },
  "type": "module",
  "scripts": {
    "start": "node .",
    "lint": "npx prettier --write . && npx eslint .",
    "lint:check": "npx prettier --check . && npx eslint .",
    "test": "npx --package=typescript tsc && npm start",
    "build": "npx --package=typescript tsc --build --clean && npx --package=typescript tsc"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "NKN1396",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.13.0"
  },
  "devDependencies": {
    "@types/node": "18.17.1",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NKN1396/nova-logs.git"
  }
}
